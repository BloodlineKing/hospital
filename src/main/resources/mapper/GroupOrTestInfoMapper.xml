<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dyhc.hospital.dao.GroupOrTestInfoMapper">
           <select id="getById" resultType="com.dyhc.hospital.entity.GroupOrTestInfo">
               select * from group_or_test_info where group_or_test_info_id=#{groupOrTestInfoId}
           </select>
           <insert id="addGroup">
               insert into group_or_test_info values (null,#{unitsGroupId},null,null,#{packageId})
           </insert>
          <update id="updateGroup">
              update group_or_test_info
              set
                units_group_id=#{unitsGroupId}
                package_id=#{packageId}
              where
                group_or_test_info_id=#{groupOrTestInfoId}
          </update>
</mapper>