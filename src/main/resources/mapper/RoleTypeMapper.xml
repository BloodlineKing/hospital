<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dyhc.hospital.dao.RoleTypeMapper">

    <select id="listRoleType" resultType="com.dyhc.hospital.entity.RoleType">
        select * from role_type
    </select>
    <select id="getById" resultType="com.dyhc.hospital.entity.RoleType">
        select * from role_type where id=#{id}
    </select>
</mapper>